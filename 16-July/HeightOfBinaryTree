import java.util.*;
import java.io.*;

class Node {
    Node left;
    Node right;
    int data;
    
    Node(int data) {
        this.data = data;
        left = null;
        right = null;
    }
}

class Solution {

	/*
    class Node 
    	int data;
    	Node left;
    	Node right;
	*/
	public static int height(Node root) {
      	// Write your code here.
        if(root == null) {
            return 0;
        } 
        if(root.left==null && root.right==null) {
            return 0; 
        }
        return Math.max(height(root.left), height(root.right)) + 1;
    }

	public static Node insert(Node root, int data) {
